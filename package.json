  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang"
  ],
  "homepage": ".",
  "license": "AGPL3.0-only",
  "dependencies": {
    "@emotion/styled": "11.8.1",
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git#44-app-key-typeorm",
    "@graasp/sdk": "v1.0.0-rc1",
    "@mui/icons-material": "5.8.2",
    "@mui/material": "5.8.2",
    "@tanstack/react-query": "4.28.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "http-status-codes": "2.2.0",
    "i18next": "21.8.8",
    "immutable": "4.1.0",
    "json2csv": "5.0.7",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "18.1.0",
    "react-csv": "2.2.2",
    "react-dom": "18.1.0",
    "react-ga": "3.3.0",
    "react-i18next": "11.17.0",
    "react-scripts": "5.0.1",
    "react-select": "5.3.2",
    "react-toastify": "9.0.3",
    "react-wordcloud": "1.2.7",
    "stopword": "2.0.2"
  },
  "scripts": {
    "start": "yarn start:local",
    "start:local": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "yarn build:react",
    "build:react": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "pre-commit": "yarn prettier:check && yarn lint",
    "eject": "react-scripts eject",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently -k -s first \"yarn start:test\" \"wait-on http://127.0.0.1:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.cy.js\""
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@tanstack/react-query-devtools": "4.28.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "concurrently": "7.2.1",
    "cross-env": "7.0.3",
    "cypress": "12.13.0",
    "env-cmd": "10.1.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "husky": "8.0.1",
    "istanbul-lib-coverage": "3.2.0",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "typescript": "5.0.4",
    "uuid": "8.3.2",
    "wait-on": "6.0.1"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/serviceWorker.js"
    ]
  },
  "resolutions": {
    "minimist": "1.2.6"
  },
  "packageManager": "yarn@3.2.1"
}
