{
  "name": "graasp-app-text-input",
  "version": "0.4.0",
  "private": true,
  "author": "React EPFL",
  "contributors": [
    "Juan Carlos Farah"
  ],
  "homepage": ".",
  "license": "MIT",
  "dependencies": {
    "@graasp/apps-query-client": "github:graasp/graasp-apps-query-client.git",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "http-status-codes": "2.2.0",
    "i18next": "21.8.8",
    "immutable": "4.1.0",
    "json2csv": "5.0.7",
    "lodash": "4.17.21",
    "miragejs": "0.1.45",
    "prop-types": "15.8.1",
    "qs": "6.10.3",
    "react": "18.1.0",
    "react-csv": "2.2.2",
    "react-dom": "18.1.0",
    "react-ga": "3.3.0",
    "react-i18next": "11.17.0",
    "react-scripts": "5.0.1",
    "react-select": "5.3.2",
    "react-toastify": "9.0.3",
    "react-wordcloud": "1.2.7",
    "stopword": "2.0.2"
  },
  "scripts": {
    "start": "yarn start:local",
    "start:local": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "run-s build:react build:bundle",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js --mode=production",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "pre-commit": "yarn prettier:check && yarn lint",
    "eject": "react-scripts eject",
    "release": "git fetch --tags && git add CHANGELOG.md && standard-version -a",
    "release:first": "git fetch --tags && git add CHANGELOG.md && standard-version -a --first-release",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome --spec \"cypress/**/*.spec.js\""
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@babel/eslint-plugin": "7.17.7",
    "@babel/preset-react": "7.17.12",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "concurrently": "7.2.1",
    "cross-env": "7.0.3",
    "cypress": "10.0.2",
    "env-cmd": "10.1.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "glob": "8.0.3",
    "husky": "8.0.1",
    "istanbul-lib-coverage": "3.2.0",
    "json-server": "0.17.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "uuid": "8.3.2",
    "wait-on": "6.0.1"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nyc": {
    "exclude": [
      "src/serviceWorker.js"
    ]
  },
  "resolutions": {
    "minimist": "1.2.6"
  },
  "packageManager": "yarn@3.2.1"
}
