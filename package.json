    "@json2csv/plainjs": "7.0.1",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.150",
    "@mui/material": "5.13.3",
    "@tanstack/react-query": "4.29.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "http-status-codes": "2.2.0",
    "i18next": "22.5.0",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-csv": "2.2.2",
