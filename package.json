{
  "name": "graasp-app-text-input",
  "version": "2.0.2",
  "private": true,
  "author": "Graasp",
  "contributors": [
    "Juan Carlos Farah",
    "Kim Lan Phan Hoang"
  ],
  "homepage": ".",
  "license": "AGPL3.0-only",
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@graasp/apps-query-client": "3.0.0",
    "@graasp/sdk": "2.0.0",
    "@graasp/ui": "4.0.0",
    "@json2csv/plainjs": "7.0.1",
    "@mui/icons-material": "5.11.16",
    "@mui/lab": "5.0.0-alpha.150",
    "@mui/material": "5.13.3",
    "@tanstack/react-query": "4.29.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "http-status-codes": "2.2.0",
    "i18next": "22.5.0",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-csv": "2.2.2",
    "react-dom": "18.2.0",
    "react-ga": "3.3.1",
    "react-i18next": "12.3.1",
    "react-router-dom": "6.17.0",
    "react-scripts": "5.0.1",
    "react-select": "5.7.3",
    "react-toastify": "9.1.3",
    "react-wordcloud": "1.2.7",
    "stopword": "2.0.8",
    "stylis": "^4.1.3",
    "stylis-plugin-rtl": "2.1.1"
  },
  "scripts": {
    "start": "yarn start:local",
    "start:local": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "yarn build:react",
    "build:react": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "pre-commit": "yarn prettier:check && yarn lint",
    "eject": "react-scripts eject",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently -k -s first \"yarn start:test\" \"wait-on http://127.0.0.1:3333 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@cypress/code-coverage": "3.10.7",
    "@cypress/instrument-cra": "1.4.0",
    "@tanstack/react-query-devtools": "4.29.12",
    "@types/lodash": "4.14.202",
    "@types/react": "18.2.8",
    "@types/react-csv": "1.1.7",
    "@types/react-dom": "18.2.4",
    "@types/stopword": "2.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "concurrently": "8.1.0",
    "cross-env": "7.0.3",
    "cypress": "12.13.0",
    "env-cmd": "10.1.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "husky": "8.0.3",
    "istanbul-lib-coverage": "3.2.0",
    "nyc": "15.1.0",
    "prettier": "2.8.8",
    "standard-version": "9.5.0",
    "typescript": "5.1.3",
    "uuid": "9.0.0",
    "wait-on": "7.0.1"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.{js,ts,jsx,tsx}"
    ],
    "exclude": [
      "src/serviceWorker.js"
    ]
  },
  "resolutions": {
    "minimist": "1.2.6"
  },
  "packageManager": "yarn@4.0.1"
}
